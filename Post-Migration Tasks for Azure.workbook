            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "resources\r\n| extend resourceType = tolower(tostring(type))\r\n| where  resourceType == \"microsoft.compute/virtualmachines\"\r\n| extend vmId = tolower(tostring(id))\r\n| join kind=leftouter (\r\n    resources\r\n    | extend resourceType = tolower(tostring(type))\r\n    | extend resourceId = tolower(tostring(id))\r\n    | where resourceType == \"microsoft.compute/virtualmachines/extensions\"\r\n    | extend extensionName = name\r\n    | extend vmId = tostring(split(resourceId, \"/extensions\")[0])\r\n    | extend provisioningState = properties.provisioningState\r\n    | extend isMDEExtensionInstalled = iff(provisioningState == \"Succeeded\" and extensionName in~ (\"MDE.Windows\", \"MDE.Linux\") and properties.publisher == \"Microsoft.Azure.AzureDefenderForServers\", 1, 0)\r\n    | extend isGuestConfigurationInstalled = iff(provisioningState == \"Succeeded\" and name in~ (\"ConfigurationforWindows\", \"ConfigurationforLinux\") and properties.publisher == \"Microsoft.GuestConfiguration\", 1, 0)\r\n    | extend isAMAInstalled = iff(provisioningState == \"Succeeded\" and name in~ (\"AzureMonitorLinuxAgent\", \"AzureMonitorWindowsAgent\") and properties.publisher == \"Microsoft.Azure.Monitor\", 1, 0)\r\n) on vmId\r\n| summarize arg_max(isMDEExtensionInstalled,isGuestConfigurationInstalled, isAMAInstalled, *) by subscriptionId, vmId, resourceGroup, location\r\n| where isMDEExtensionInstalled != 1 or isGuestConfigurationInstalled != 1 or isAMAInstalled != 1\r\n| project subscriptionId, vmId, resourceGroup, location, isMDEExtensionInstalled, isGuestConfigurationInstalled, isAMAInstalled",
